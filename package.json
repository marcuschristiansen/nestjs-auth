{
    "name": "microservice-template",
    "version": "0.6.9",
    "description": "",
    "author": "Your boi",
    "private": true,
    "license": "Test License",
    "scripts": {
        "redis-commander": "docker run --rm --name redis-commander -p 8081:8081 --env REDIS_HOST=host.docker.internal rediscommander/redis-commander:latest",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "eslint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
        "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx|scss|css|html)\"",
        "format": "npm run prettier -- --write",
        "format:check": "npm run prettier -- --check",
        "lint": "npm run eslint -- --fix",
        "lint:check": "npm run eslint",
        "validate": "npm-run-all --parallel format:check lint:check"
    },
    "dependencies": {
        "@azure/storage-file": "^10.3.0",
        "@azure/storage-file-share": "^12.1.2",
        "@nestjs/azure-storage": "^2.1.1",
        "@nestjs/common": "^7.0.0",
        "@nestjs/config": "^0.5.0",
        "@nestjs/core": "^7.0.0",
        "@nestjs/jwt": "^7.1.0",
        "@nestjs/microservices": "^7.1.3",
        "@nestjs/platform-express": "^7.0.0",
        "@nestjs/schedule": "^0.4.0",
        "@nestjs/swagger": "^4.6.1",
        "@nestjs/typeorm": "^7.1.4",
        "@types/bcryptjs": "^2.4.2",
        "amqp-connection-manager": "^3.2.0",
        "amqplib": "^0.5.6",
        "bcrypt": "^5.0.0",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.12.2",
        "dotenv": "^8.2.0",
        "ioredis": "^4.17.3",
        "iorejson": "^0.1.1",
        "nest-router": "^1.0.9",
        "nestjs-redis": "^1.2.8",
        "pg": "^8.4.0",
        "redis": "^3.0.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "swagger-ui-express": "^4.1.4",
        "typeorm": "^0.2.28",
        "typeorm-seeding": "^1.6.1",
        "uuid": "^8.0.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.0.0",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.4.4",
        "@types/bcrypt": "^3.0.0",
        "@types/express": "^4.17.3",
        "@types/faker": "^5.1.2",
        "@types/jest": "25.2.3",
        "@types/node": "^13.9.1",
        "@types/redis": "^2.8.22",
        "@types/supertest": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "^3.3.0",
        "@typescript-eslint/parser": "^3.3.0",
        "eslint": "^7.3.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.1",
        "jest": "26.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "supertest": "^4.0.2",
        "ts-jest": "26.1.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^6.1.2",
        "typescript": "^3.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
